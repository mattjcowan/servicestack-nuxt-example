{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf ./.nuxt & rimraf ./dist",
    "server": "cd ../backend/WebApp && dotnet restore && dotnet build && dotnet run",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "npm run clean && nuxt generate && cpx \"dist/**/*.*\"  ../backend/WebApp/wwwroot",
    "lint:js": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "lint": "npm run lint:js"
  },
  "dependencies": {
    "@nuxtjs/auth": "^4.9.1",
    "@nuxtjs/axios": "^5.12.2",
    "core-js": "^3.6.5",
    "normalize.css": "^8.0.1",
    "nuxt": "^2.14.5"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "cpx": "^1.5.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "node-sass": "^7.0.0",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.0.2"
  }
}
